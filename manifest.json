{
  "manifest_version": 3,
  "version": "2.0.5.8",
  "name": "Better Lyrics (Lyrics for Youtube Music)",
  "description": "Enhance YouTube Music with beautiful time-synced lyrics, multilingual support, real-time translations and more",
  "homepage_url": "https://better-lyrics.boidu.dev",
  "icons": {
    "16": "images/icons/icon-16.png",
    "48": "images/icons/icon-48.png",
    "512": "images/icons/icon-512.png"
  },
  "chrome:key": "",
  "host_permissions": [
    "*://music.youtube.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "downloads"
  ],
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": false
  },
  "web_accessible_resources": [
    {
      "matches": [
        "*://music.youtube.com/*"
      ],
      "resources": [
				"css/blyrics.css",
        "css/ytmusic.css",
        "css/disablestylizedanimations.css",
        "css/themesong.css",
        "css/themes/*.css",
        "*.js.map",
        "src/options/standalone-editor.html"
      ]
    }
  ],
  "chromium:background": {
    "service_worker": "src/options/background.ts",
    "type": "module"
  },
  "gecko:background": {
    "scripts": [
      "src/options/background.ts"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "betterlyrics@boidu.dev",
      "strict_min_version": "100.0"
    }
  },
  "chrome:update_url": "https://clients2.google.com/service/update2/crx",
  "chromium:key": "",
  "content_scripts": [
    {
      "matches": [
        "*://music.youtube.com/*"
      ],
      "js": [
        "src/index.ts"
      ],
      "run_at": "document_start",
      "world": "ISOLATED"
    },
    {
      "matches": [
        "*://music.youtube.com/*"
      ],
      "js": [
        "public/earlyInject.js"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    },
    {
      "matches": [
        "*://music.youtube.com/*"
      ],
      "js": [
        "public/script.js"
      ],
      "run_at": "document_end",
      "world": "MAIN"
    }
  ],
  "action": {
    "default_popup": "src/options/options.html"
  },
  "version_name": "2.0.5-canary"
}
